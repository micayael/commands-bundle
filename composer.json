{
  "name": "micayael/commands-bundle",
  "description": "Useful commands for Symfony4.1+",
  "type": "symfony-bundle",
  "keywords": [
    "command"
  ],
  "homepage": "https://github.com/micayael/commands-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Juan Ardissone",
      "email": "micayael@gmail.com",
      "homepage": "http://micayael.com"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "symfony/http-kernel": "^4.1",
    "symfony/dependency-injection": "^4.1",
    "symfony/console": "^4.1",
    "symfony/config": "^4.1",
    "symfony/finder": "^4.1",
    "symfony/translation": "^4.1",
    "symfony/yaml": "^4.1"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.12",
    "symfony/framework-bundle": "^4.1",
    "symfony/var-dumper": "^4.1"
  },
  "autoload": {
    "psr-4": {
      "Micayael\\CommandsBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Micayael\\CommandsBundle\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  }
}
