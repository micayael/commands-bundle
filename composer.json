{
  "name": "micayael/commands-bundle",
  "type": "symfony-bundle",
  "description": "Useful commands for Symfony2+",
  "keywords": ["command"],
  "homepage": "https://github.com/micayael/commands-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Juan Ardissone",
      "email": "micayael@gmail.com",
      "homepage": "http://micayael.com"
    }
  ],
  "require": {
    "php": ">=5.3.2",
    "symfony/config": "^3.2",
    "symfony/console": "^3.2",
    "symfony/dependency-injection": "^3.2",
    "symfony/finder": "^3.2",
    "symfony/http-kernel": "^3.2"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.3",
    "phpunit/phpunit": "^6.1"
  },
  "autoload": {
    "psr-4": { "Micayael\\CommandsBundle\\": "" }
  },
  "config": {
    "sort-packages": true
  }
